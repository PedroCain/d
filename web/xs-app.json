  "destination": "versions-xsjs-backend",
  "authenticationType": "xsuaa"

},
{
  "source": "(.*)/ERPIBERIA_ADN/tables/(.*)(.xsjs)",
  "destination": "tables-xsjs-backend",
  "csrfProtection": false,
  "authenticationType": "xsuaa"
}, {
  "source": "(.*)/ERPIBERIA_ADN/tables/(.*)(.xsodata)",
  "destination": "tables-xsjs-backend",
  "authenticationType": "xsuaa"

}
]

}